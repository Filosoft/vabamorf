            "program": "${workspaceFolder}/../project/unix/vmetajson.db",
            "cwd": "${workspaceFolder}",
            "args": [ "--guesspropnames", "--sentences"],
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
